<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
>
  <system:String x:Key="ThemeName">Theme.Purple</system:String>
  <SolidColorBrush x:Key="ThemeColor" Color="#FFD7AFFF"/>
  <SolidColorBrush x:Key="Background" Color="#FFF2E6FF"/>
  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FFE7CFFF"/>
  <SolidColorBrush x:Key="Foreground" Color="#FF000000"/>
  <SolidColorBrush x:Key="ForegroundSecondary" Color="#FF616161"/>
  <SolidColorBrush x:Key="SelectionBackground" Color="#FF9159C7"/>
  <SolidColorBrush x:Key="TextCursorForeground" Color="#FF000000"/>
</ResourceDictionary>
