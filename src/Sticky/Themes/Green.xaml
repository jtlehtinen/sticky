<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
>
  <system:String x:Key="ThemeName">Theme.Green</system:String>
  <SolidColorBrush x:Key="ThemeColor" Color="#FFA1EF9B"/>

  <SolidColorBrush x:Key="Background" Color="#FFE4F9E0"/>
  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FFCBF1C4"/>
  <SolidColorBrush x:Key="BackgroundHover" Color="#FFD5E8D1"/>
  <SolidColorBrush x:Key="Foreground" Color="#FF000000"/>
  <SolidColorBrush x:Key="ForegroundSecondary" Color="#FF616161"/>
  <SolidColorBrush x:Key="SelectionBackgroundColor" Color="#FF3C862C"/>
</ResourceDictionary>
