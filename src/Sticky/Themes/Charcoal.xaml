<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
>
  <system:String x:Key="ThemeName">Theme.Charcoal</system:String>
  <SolidColorBrush x:Key="ThemeColor" Color="#FF767676"/>

  <SolidColorBrush x:Key="Background" Color="#FF696969"/>
  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FF494745"/>
  <SolidColorBrush x:Key="BackgroundHover" Color="#FF737373"/>
  <SolidColorBrush x:Key="Foreground" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="ForegroundSecondary" Color="#FFCCCCCC"/>
  <SolidColorBrush x:Key="SelectionBackgroundColor" Color="#FF767676"/>
</ResourceDictionary>
