  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib"
>
  <!-- AutoSuggestBox -->
  <Color x:Key="AutoSuggestBoxBackground">#FFEEEEEE</Color>
  <Color x:Key="AutoSuggestBoxPlaceholderForeground">#FF666666</Color>
