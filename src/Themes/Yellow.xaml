<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
>
  <system:String x:Key="ThemeName">Theme.Yellow</system:String>
  <SolidColorBrush x:Key="ThemeColor" Color="#FFFFE66E"/>

  <SolidColorBrush x:Key="Background" Color="#FFFFF7D1"/>
  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FFFFF2AB"/>
  <SolidColorBrush x:Key="BackgroundHover" Color="#FFEEE7C3"/>
  <SolidColorBrush x:Key="Foreground" Color="#FF000000"/>
  <SolidColorBrush x:Key="ForegroundSecondary" Color="#FF616161"/>
  <SolidColorBrush x:Key="SelectionBackgroundColor" Color="#FF8F7300"/>

  <SolidColorBrush x:Key="TitleBar.Background.MouseOver" Color="#FFF3F3F3"/>
  <SolidColorBrush x:Key="TitleBar.Background.Pressed" Color="#FFDCDCDC"/>
  <SolidColorBrush x:Key="TitleBar.Foreground" Color="#FF616161"/>
</ResourceDictionary>
