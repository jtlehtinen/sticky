  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FFE1DFDD"/>
  <SolidColorBrush x:Key="BackgroundHover" Color="#FFE3E2E1"/>
  <SolidColorBrush x:Key="Foreground" Color="#FF000000"/>
