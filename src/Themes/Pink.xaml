  <SolidColorBrush x:Key="BackgroundSecondary" Color="#FFFFCCE5"/>
  <SolidColorBrush x:Key="BackgroundHover" Color="#FFEED5E1"/>
  <SolidColorBrush x:Key="Foreground" Color="#FF000000"/>
